package softeng;
import java.io.Serializable;

public class Movie implements Serializable {
   private String name, dir, genre;
   private int year;
   public Movie(String name, String dir, String genre, int year) {
      this.name = name; this.dir = dir; this.genre= genre; this.year=year;
   }
   public String getName() {
      return "제목 : "+name+" , ";
   }
   public void setName(String name) {
      this.name = name;
   }
   public String getDir() {
      return "감독 : "+dir+" , ";
   }
   public void setDir(String dir) {
      this.dir = dir;
   }
   public String getGenre() {
      return "장르 : "+genre+" , ";
   }
   public void setGenre(String genre) {
      this.genre = genre;
   }
   public String getYear() {
      return "개봉년도 : "+year;
   }
   public void setYear(int year) {
      this.year = year;
   }
   
}